<schema targetNamespace="http://www.tibco.com/schemas/TIBCO_BW_REST_EXAMPLE/Schemas/Schema.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<!-- Global Element Type Definitions -->
	<element name="ExchangeRate" type="xs:decimal"/>
	<element name="AcctNumber" type="xs:string"/>
	<element name="AmountType" type="xs:decimal"/>
	<element name="IdType" type="xs:string"/>

	<!-- Global Element Definitions -->
	<element name="TxnInfo">
		<complexType>
			<sequence>
				<element ref="IdType"/>
				<element ref="AmountType"/>
				<element ref="AcctNumber"/>
				<element name="Currency" type="xs:string"/>
				<element name="TxnDate" type="xs:string"/>
			</sequence>
		</complexType>
	</element>

	<element name="BranchInfo">
		<complexType>
			<sequence>
				<element ref="IdType" minOccurs="0"/>
				<element name="ChannelType" type="xs:string" minOccurs="0"/>
			</sequence>
		</complexType>
	</element>

	<!-- Main Element using refs -->
	<element name="AcctDetails">
		<complexType>
			<sequence>
				<element name="RecordIndicator" type="xs:string"/>
				<element ref="IdType"/>
				<element ref="AcctNumber"/>
				<element ref="TxnInfo"/>
				<element name="TxnTypeCode" type="xs:string" minOccurs="0"/>
				<element name="TxnTime" type="xs:string"/>
				<element ref="BranchInfo"/>
				<element name="StatusCode" type="xs:string" minOccurs="0"/>
				<element name="RefNumber" type="xs:string" minOccurs="0"/>
				<element ref="AmountType"/>
				<element name="ProcessDate" type="xs:string"/>
				<element name="MerchantCode" type="xs:string" minOccurs="0"/>
				<element name="AuthCode" type="xs:string" minOccurs="0"/>
				<element ref="AmountType" minOccurs="0"/>
				<element ref="ExchangeRate" minOccurs="0"/>
				<element name="Filler" type="xs:string" minOccurs="0"/>
			</sequence>
		</complexType>
	</element>
</schema>